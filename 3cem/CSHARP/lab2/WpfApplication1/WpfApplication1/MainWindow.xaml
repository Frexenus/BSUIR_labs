        <Label Content="difficulty" HorizontalAlignment="Left" Margin="321,38,0,0" VerticalAlignment="Top" Foreground="#FFD3D3C3"/>
        <Label Content="8.2" HorizontalAlignment="Left" Margin="255,0,0,0" VerticalAlignment="Top" Foreground="White" FontSize="45" FontFamily="Segoe UI Light" FontWeight="Bold"/>
