        <Button Content="Autoadd" Margin="10,264,307,20" Background="#FF333337" Foreground="#FFF5FFFF"/>
        <Button Content="START" Margin="382,263,20,21" Background="#FFFFCC00" FontSize="15"/>
        <Button Content="ADD" Margin="10,235,0,0" Click="Button_Click" Foreground="Black" Background="#FFFFCC00" HorizontalAlignment="Left" Width="200" Height="24" VerticalAlignment="Top"/>
        <Label Content="5" HorizontalAlignment="Left" Margin="396,0,0,0" VerticalAlignment="Top" Foreground="White" FontSize="45" FontFamily="Segoe UI Light" FontWeight="Bold"/>
        <Label Content="questions" HorizontalAlignment="Left" Margin="429,22,0,0" VerticalAlignment="Top" Foreground="LightGray" FontSize="15" Height="32"/>
        <Label Content="available" HorizontalAlignment="Left" Margin="429,38,0,0" VerticalAlignment="Top" Foreground="#FFD3D3C3"/>
