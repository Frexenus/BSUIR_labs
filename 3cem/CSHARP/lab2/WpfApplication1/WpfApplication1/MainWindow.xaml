        <Button Content="Show" Margin="290,263,140,20" Background="#FF333337" Foreground="White"/>
        <TextBox HorizontalAlignment="Left" Height="26" Margin="10,57,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="155" Background="#FF333337" Foreground="White" FontFamily="Segoe UI Semibold" FontSize="15"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="46,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="164" Background="#FF333337" Foreground="White"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="46,117,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="164" Background="#FF333337" Foreground="White"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="46,148,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="164" Background="#FF333337" Foreground="White"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="46,177,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="164" Background="#FF333337" Foreground="White"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="46,206,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="164" Background="#FF333337" Foreground="White"/>
        <Label Content="1." HorizontalAlignment="Left" Margin="15,88,0,0" VerticalAlignment="Top" Foreground="#FF787878"/>
        <Label Content="2." HorizontalAlignment="Left" Margin="15,115,0,0" VerticalAlignment="Top" Foreground="#FF787878"/>
        <Label Content="3." HorizontalAlignment="Left" Margin="15,146,0,0" VerticalAlignment="Top" Foreground="#FF787878"/>
        <Label Content="4." HorizontalAlignment="Left" Margin="15,177,0,0" VerticalAlignment="Top" Foreground="#FF787878"/>
        <Label Content="5." HorizontalAlignment="Left" Margin="15,203,0,0" VerticalAlignment="Top" Foreground="#FF787878"/>
        <ComboBox HorizontalAlignment="Left" Height="26" Margin="170,57,0,0" VerticalAlignment="Top" Width="40" Foreground="#FF383838" BorderBrush="{x:Null}" Background="{x:Null}">
